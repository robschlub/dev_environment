{
    "extends": ["plugin:flowtype/recommended", "plugin:jest/recommended","airbnb-base"],
    "rules": {
        "no-console": "error"
        // "quotes": ["error", "double"]
      },
    "plugins": [
      "flowtype",
      "jest"],

    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      },
    },
    "env": {
      "browser": true
    }
}